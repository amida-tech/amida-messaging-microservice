{
  "name": "amida-messaging-microservice",
  "version": "2.5.0",
  "description": "Microservice for a lightweight messaging application",
  "author": "Jacob Sachs <jacob@amida.com>",
  "main": "dist/index.js",
  "private": false,
  "engines": {
    "node": ">=8.14.0",
    "npm": ">=6.4.0",
    "yarn": ">=1.12.0"
  },
  "scripts": {
    "build": "gulp",
    "create_db": "sequelize db:create",
    "delete_db": "sequelize db:drop",
    "jenkins": "cross-env NODE_ENV=test yarn delete_db; cross-env NODE_ENV=test yarn create_db && cross-env NODE_ENV=test yarn migrate && cross-env NODE_ENV=test yarn test",
    "lint": "esw -c .eslintrc --color .",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "prepublish": "snyk protect",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "serve": "node dist/index.js",
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=amida-messaging-microservice:* yarn start",
    "test": "cross-env NODE_ENV=test mocha --opts .mocharc",
    "test:check-coverage": "yarn test:coverage && nyc check-coverage",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --opts .mocharc",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "bluebird": "3.4.6",
    "body-parser": "1.18.2",
    "chai": "^4.1.2",
    "compression": "1.7.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "del": "^2.2.0",
    "dotenv": "^4.0.0",
    "express": "4.16.0",
    "express-validation": "1.0.1",
    "express-winston": "2.5.0",
    "gulp": "3.9.1",
    "gulp-load-plugins": "^1.2.0",
    "helmet": "3.6.1",
    "http-status": "^0.2.0",
    "joi": "10.0.6",
    "jsonwebtoken": "8.0.0",
    "lodash": "4.17.5",
    "method-override": "3.0.0",
    "node-rest-client": "3.1.0",
    "nyc": "false13.3.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "pg": "6.1.6",
    "pg-hstore": "2.3.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "run-sequence": "^1.1.5",
    "sequelize": "^4.28.6",
    "sequelize-cli": "^5.2.0",
    "snyk": "^1.69.4",
    "supertest": "^3.0.0",
    "supertest-as-promised": "4.0.2",
    "swagger-stats": "^0.95.6",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "winston-json-formatter": "^0.9.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "chai-date-string": "0.1.0",
    "chai-datetime": "1.5.0",
    "coveralls": "3.0.2",
    "cross-env": "3.1.3",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "gulp-babel": "6.1.2",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "husky": "1.2.1",
    "mocha": "3.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "snyk": true
}
